@model IEnumerable<TravelWebApp.Models.Enquiry>

@{
    ViewData["Title"] = "UserEnquiries";
}

<div class="container">
    <div class="text-center">
        <h1 class="display-4">My Enquiries</h1>
    </div>
    <hr />
    <table class="table table-bordered">
        <thead class="thead-dark">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Phone)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.About)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EnquiryDate)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Phone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.About)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EnquiryDate)
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>